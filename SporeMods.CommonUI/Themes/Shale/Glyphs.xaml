<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
            xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
            xmlns:cui="clr-namespace:SporeMods.CommonUI"
            xmlns:e="http://schemas.microsoft.com/expression/2010/interactivity"
            xmlns:sys="clr-namespace:System;assembly=mscorlib">
    <PathGeometry x:Key="Arrow!Left!Small">
        M 7 0 L 7 2 L 4 5
        L 7 8 L 7 10 L 5 10
        L 0 5 L 5 0 Z
    </PathGeometry>
    <PathGeometry x:Key="Arrow!Up!Small">
        M 0 7 L 2 7 L 5 4
        L 8 7 L 10 7 L 10 5
        L 5 0 L 0 5 Z
    </PathGeometry>
    <PathGeometry x:Key="Arrow!Right!Small">
        M 0 0 L 0 2 L 3 5
        L 0 8 L 0 10 L 2 10
        L 7 5 L 2 0 Z
    </PathGeometry>
    <PathGeometry x:Key="Arrow!Down!Small">
        M 0 0 L 2 0 L 5 3
        L 8 0 L 10 0 L 10 2
        L 5 7 L 0 2 Z
    </PathGeometry>

    <PathGeometry x:Key="Spyglass">
        M 10.5 0
        A 5.5 5.5 0 0 0 5 5.5
        A 5.5 5.5 0 0 0 5.7421875 8.2578125
        L 0 14 L 0 16 L 2 16 L 7.7421875 10.257812
        A 5.5 5.5 0 0 0 10.5 11
        A 5.5 5.5 0 0 0 16 5.5
        A 5.5 5.5 0 0 0 10.5 0 z

        M 10.5 2
        A 3.5 3.5 0 0 1 14 5.5
        A 3.5 3.5 0 0 1 10.5 9
        A 3.5 3.5 0 0 1 7 5.5
        A 3.5 3.5 0 0 1 10.5 2 z
    </PathGeometry>

    <!--Close-->
    <PathGeometry x:Key="DiagonalCross!Small">
        M 0 -1 L 2 -1 L 4.5 2 L 7 -1 L 9 -1 L 9 1 L 6 3.5 L 9 6 L 9 8 L 7 8 L 4.5 5 L 2 8 L 0 8 L 0 6 L 3 3.5 L 0 1 Z
    </PathGeometry>
    <!--M 0 -1 L 2 -1 L 4.5 2 L 7 -1 L 9 -1 L 9 1 L 6 3.5 L 9 6 L 9 8 L 7 8 L 4.5 5 L 2 8 L 0 8 L 0 6 L 3 3.5 L 0 1 Z-->
    <!--Cancel-->
    <PathGeometry x:Key="DiagonalCross!Large">
        M 0 2 L 0 0 L 2 0 L 8 6 L 14 0 L 16 0
        L 16 2 L 10 8 L 16 14 L 16 16
        L 14 16 L 8 10 L 2 16 L 0 16
        L 0 14 L 6 8 Z
    </PathGeometry>
    <!--M 0 0 L 16 16 M 0 16 L 16 0-->

    
</ResourceDictionary>
