  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <UseWPF>true</UseWPF>
    <AssemblyTitle>SporeMods.Manager</AssemblyTitle>
    <Product>SporeMods.Manager</Product>
  </PropertyGroup>
  <Import Project="..\SporeMods.SharedOutputDirs.props" />
