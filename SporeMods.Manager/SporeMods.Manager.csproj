  <ItemGroup>
    <PackageReference Include="WPFTextBoxAutoComplete" Version="1.0.5" />
  </ItemGroup>
  <ItemGroup>
    <Content Include="..\AppIcons\ModManagerIcon.ico">
      <Link>%(RecursiveDir)%(Filename)%(Extension)</Link>
      <LogicalName>ModManagerIcon.ico</LogicalName>
    </Content>
  </ItemGroup>
