<Application x:Class="SporeMods.Setup.App"
			 xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
			 xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
			 xmlns:local="clr-namespace:SporeMods.Setup">
	<Application.Resources>
		<ResourceDictionary>
			<ResourceDictionary.MergedDictionaries>
				<!--ResourceDictionary Source="pack://application:,,,/component/Locale/en-ca.xaml"/-->
			</ResourceDictionary.MergedDictionaries>

            <Style x:Key="{x:Type Window}" TargetType="{x:Type Window}">
                <Setter Property="FontFamily" Value="pack://application:,,,/Fonts/#Ubuntu"/>
            </Style>
		</ResourceDictionary>
	</Application.Resources>
</Application>
